# Audio
/dev/socket/audio_hw_socket                                                                            u:object_r:audio_socket:s0

# Camera
/mnt/vendor/persist/camera(/.*)?                                                                       u:object_r:camera_persist_file:s0

# Fingerprint
/dev/goodix_fp                                                                                         u:object_r:fingerprint_device:s0
/data/vendor/goodix(/.*)?                                                                              u:object_r:fingerprint_data_file:s0

# FOD
/sys/class/touch/tp_dev/fod_status				u:object_r:sysfs_fod:s0
/sys/devices/platform/soc/4a88000.i2c/i2c-1/1-0038/touch/tp_dev/fod_status          u:object_r:sysfs_fod:s0
/sys/devices/platform/soc/5e00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/disp_param    u:object_r:sysfs_fod:s0

# HALs
/(vendor|system/vendor)/bin/hw/android\.hardware\.light@2\.0-service\.laurel_sprout                                   u:object_r:hal_light_default_exec:s0
/vendor/bin/hw/vendor\.pa\.biometrics\.fingerprint\.inscreen@1.0-service\.xiaomi_laurel_sprout         u:object_r:hal_pa_fod_default_exec:s0

# IR
/dev/spidev[0-9]\.1                                                                                             u:object_r:ir_dev_file:s0

# Remosaic
/vendor/bin/remosaic_daemon                                                                            u:object_r:remosaic_daemon_exec:s0

# Sensors
/dev/elliptic(.*)?                                                                                     u:object_r:sound_device:s0

# WiFi
/vendor/bin/nv_mac                                                                                     u:object_r:wcnss_service_exec:s0
/vendor/bin/wifi-mac-generator                                                                         u:object_r:wifi-mac-generator_exec:s0

# Xiaomi thermal daemon
/vendor/bin/mi_thermald		                                                                       u:object_r:mi_thermald_exec:s0
/data/vendor/thermal(/.*)?                                                                             u:object_r:thermal_data_file:s0
