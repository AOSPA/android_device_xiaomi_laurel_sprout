/sys/devices/platform/soc/5e00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/disp_param               u:object_r:sysfs_fod:s0
/sys/class/touch/tp_dev/fod_status                                                             u:object_r:sysfs_fod:s0
