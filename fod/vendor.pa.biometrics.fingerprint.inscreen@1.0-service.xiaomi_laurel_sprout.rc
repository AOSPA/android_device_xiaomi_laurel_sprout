on boot
    chown system system /sys/class/touch/tp_dev/fod_status
    chmod 0644 /sys/class/touch/tp_dev/fod_status

service fingerprint-inscreen-1-0 /vendor/bin/hw/vendor.pa.biometrics.fingerprint.inscreen@1.0-service.xiaomi_laurel_sprout
    interface vendor.pa.biometrics.fingerprint.inscreen@1.0::IFingerprintInscreen default
    class hal
    user system
    group system
    shutdown critical
